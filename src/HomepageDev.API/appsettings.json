{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "AppOptions": {
      "Environment": "Development"
    },
    "GeocodeOptions": {
      "Bing": {
        "ApiRootUrl": "https://dev.virtualearth.net/",
        "GeocodeSingleAddressEndpoint": "REST/v1/Locations",
        "MaxResults": 20,
        "IncludeNeighborhood": 1
      }
    }
  }
}
